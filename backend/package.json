{
  "name": "book-rag-backend",
  "version": "1.0.0",
  "description": "Backend API for Book RAG authentication and RAG pipeline",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev:all": "concurrently \"npm start\" \"npm run dev --prefix ..\"",
    "type-check": "tsc --noEmit",
    "ingest": "tsx src/scripts/run-ingestion.ts"
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@neondatabase/serverless": "^1.0.2",
    "@qdrant/js-client-rest": "^1.13.0",
    "better-auth": "^1.2.8",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "express": "^5.2.1",
    "gray-matter": "^4.0.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.0",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.18.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
