{
  "name": "book-rag",
  "version": "1.0.0",
  "description": "AI-Powered Physical AI & Robotics Textbook with RAG",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "setup": "node scripts/check-env.js && npm install && cd backend && npm install",
    "dev:all": "concurrently \"npm run start\" \"cd backend && npm run dev\"",
    "db:generate": "cd backend && npm run db:generate",
    "db:push": "cd backend && npm run db:push"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@google/generative-ai": "0.24.1",
    "@mdx-js/react": "^3.0.0",
    "@neondatabase/serverless": "^1.0.2",
    "better-auth": "^1.2.2",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.38.3",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.31.7",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}